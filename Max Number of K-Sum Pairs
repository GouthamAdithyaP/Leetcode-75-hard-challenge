from collections import defaultdict
class Solution:
    def maxOperations(self, nums: List[int], k: int) -> int:
        freq = defaultdict(int)
        operations = 0
        for num in nums:
            if freq[k - num] > 0:
                operations += 1
                freq[k - num] -= 1
            else:
                freq[num] += 1
        
        return operations
